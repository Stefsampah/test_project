    .reward-ultime { background: linear-gradient(135deg, #fbbf24 0%, #f59e0b 50%, #d97706 100%); }
    .reward-premium { background: linear-gradient(135deg, #c0c0c0 0%, #a8a8a8 50%, #808080 100%); }
    .reward-exclusif { background: linear-gradient(135deg, #cd7f32 0%, #b87333 50%, #9c6b2a 100%); }
    .reward-challenge { background: linear-gradient(135deg, #fb923c 0%, #f97316 50%, #ea580c 100%); }
    
    /* Bordure d√©corative */
    .decorative-border {
      position: absolute;
      inset: 0;
      border: 3px solid;
      border-radius: 16px;
      pointer-events: none;
    }
    
    .border-or { border-color: rgba(251, 191, 36, 0.8); }
    .border-ultime { border-color: rgba(251, 191, 36, 0.8); }
    .border-premium { border-color: rgba(192, 192, 192, 0.8); }
